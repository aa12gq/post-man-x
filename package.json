{
  "name": "rpc-postman",
  "private": true,
  "version": "0.0.1",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist dist-electron && tsc -p electron/tsconfig.json && vue-tsc && vite build && electron-builder",
    "electron:dev": "vite --mode development",
    "electron:build": "vue-tsc && vite build && electron-builder"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@grpc/grpc-js": "^1.12.4",
    "@grpc/proto-loader": "^0.7.13",
    "@types/uuid": "^10.0.0",
    "@vscode/codicons": "^0.0.36",
    "axios": "^1.6.7",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "element-plus": "^2.5.6",
    "monaco-editor": "^0.45.0",
    "nanoid": "^5.0.9",
    "pinia": "^2.3.0",
    "pinia-plugin-persistedstate": "^4.1.3",
    "uuid": "^11.0.3",
    "vue": "^3.4.15",
    "vue-i18n": "11.0.0-beta.2",
    "vue-router": "^4.5.0",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@types/vue-i18n": "^7.0.0",
    "@types/vue-router": "^2.0.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "electron": "^28.3.3",
    "electron-builder": "^24.9.1",
    "rimraf": "^5.0.10",
    "typescript": "^5.3.3",
    "vite": "^5.1.0",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vue-tsc": "^2.0.4"
  },
  "build": {
    "appId": "com.rpc.postman",
    "productName": "RPC Master",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "build/icons/icon.png"
    }
  }
}
